{
  "name": "jp-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^2.6.0",
    "@date-io/moment": "^2.6.0",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@types/c3": "^0.7.4",
    "autosuggest-highlight": "^3.1.1",
    "babel-plugin-import": "^1.13.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.12.0",
    "env-cmd": "^10.1.0",
    "exceljs": "^3.8.2",
    "file-saver": "^2.0.2",
    "install": "^0.13.0",
    "jest-junit": "^10.0.0",
    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.4.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "material-table": "^1.57.2",
    "material-ui-chip-input": "^1.1.0",
    "material-ui-dropzone": "^2.5.0",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^1.5.2",
    "mobx-utils": "^5.5.7",
    "moment": "^2.24.0",
    "npm": "^6.14.4",
    "react": "^16.13.1",
    "react-adal": "^0.5.0",
    "react-autosuggest": "^9.4.3",
    "react-c3js": "^0.1.20",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "recharts": "^1.8.5",
    "semantic-ui-css": "^2.4.1",
    "typeface-roboto": "0.0.75",
    "typescript": "3.8.2",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "start": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling react-scripts start",
    "build": "react-scripts build",
    "build-qa": "env-cmd -f .env.qa npm run build",
    "build-production": "env-cmd -f .env.production npm run build",
    "test": "react-scripts test --env=jsdom",
    "test-ci": "cross-env CI=true react-scripts test --env=jsdom --ci --reporters=jest-junit --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/autosuggest-highlight": "^3.1.0",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "25.1.3",
    "@types/jspdf": "^1.3.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "13.7.4",
    "@types/react": "16.9.21",
    "@types/react-adal": "^0.4.2",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "^5.1.4",
    "@types/recharts": "^1.8.9",
    "tar": "^6.0.1"
  },
  "jest": {
    "coverageReporters": [
      "cobertura",
      "html"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "test/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
